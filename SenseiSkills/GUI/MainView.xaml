<UserControl
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ut="clr-namespace:SenseiSkills.GUI"
             mc:Ignorable="d" Foreground="{DynamicResource TextBrush}" Background="#434343"
             MinHeight="650" MinWidth="550">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- <ResourceDictionary Source="./Dictionary.xaml"/> -->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <TabControl>
            <TabItem Header="General Settings" Foreground="{DynamicResource TextBrush}">
             
            </TabItem>
            <TabItem Header="Skill Editor" Foreground="{DynamicResource TextBrush}">
                <Border Tag="TreeView.xaml"/>
            </TabItem>
        </TabControl>

    </Grid>
</UserControl>
